        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty lca:single_score ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty lca:normalized_value ],
        [ a owl:Restriction ;
