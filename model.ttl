            owl:allValuesFrom xsd:float ;
            owl:onProperty lca:normalized_value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty lca:weighted_result ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty lca:single_score_unit ],
        [ a owl:Restriction ;
